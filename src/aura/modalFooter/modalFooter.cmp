<aura:component description="modalFooter" controller="PersonAccountsController" implements="forceCommunity:availableForAllPageTypes">
    <lightning:overlayLibrary aura:id="overlayLib"/>
    <lightning:navigation aura:id="navigationService" />
    <aura:registerEvent name="ClickOkModalEvent" type="c:OnClickOkModalEvent"/>
    <aura:attribute name="account" type="sObject"/>
    <lightning:button name="cancel" label="Cancel" onclick="{!c.handleCancel}"/>
    <lightning:button name="ok" label="OK" variant="brand" onclick="{!c.handleOK}"/>
</aura:component>
